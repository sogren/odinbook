#signing
  %h2 Log in
  = simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
    .form-inputs
      = f.input :email, required: true
      = f.input :password, required: true
    .actions
      = f.button :submit, "Log in"
  = render "devise/shared/links"
