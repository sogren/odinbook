<% if user_signed_in? %><div id="main-profile-wrapper" class="clearfix">

	<%= render "static_pages/header" %>

	<%= render "static_pages/profile" %>

	<div id="dashboard-posts">
		<div class="new-post">
			<%= render partial: "posts/form" %>
		</div>
		<div class="posts">
			<%= render @posts %>
		</div>
	</div>

	<div id="you-may-know">
		<%= render @users %>
	</div>
</div>

<% else %>

	<%= render "static_pages/welcome" %>

<% end %>