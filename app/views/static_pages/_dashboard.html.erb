<div id="main-profile-wrapper">

	<div id="header">
		<p id="logo">odinbook</p>
		
		<%= link_to "log out", destroy_user_session_path, :method => :delete %>
	</div>

	<div class="profile">
	</div>

	<div id="post-wrapper">
		<div class="new-post">
			<%= render partial: "posts/form" %>
		</div>
	
		<div class="posts">
			<%= render @posts %>
		</div>
	</div>

	<div id="people">
		<%= render @users %>
	</div>

</div>